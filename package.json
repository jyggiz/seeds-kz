  "scripts": {
    "dev": "webpack serve --config build-tools/config/webpack/webpack.conf.dev.js",
    "dev:code": "webpack --config build-tools/config/webpack/webpack.conf.code.dev.js --info-verbosity none --watch",
    "build": "cross-env NODE_ENV=production node --unhandled-rejections=strict ./build-tools/script/util/content/index & cross-env NODE_ENV=production node --unhandled-rejections=strict ./build-tools/script/build",
    "content": "cross-env NODE_ENV=production node --unhandled-rejections=strict ./build-tools/script/util/content/index",
    "build:dev": "npm run build dev",
    "build:diff": "cross-env node ./build-tools/script/diff2html",
    "preview": "node build-tools/script/preview-server.js",
    "compile:code": "npm run build code",
    "compile:partials": "npm run build partials",
    "compile:html": "npm run build html",
    "clean": "npm run build clean",
    "clean:boilerplate": "node ./build-tools/script/cleanup",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "npm-run-all lint:*",
    "lint:js": "eslint src --ext .js,.ts --cache",
    "lint:css": "stylelint src/**/*.scss --cache",
    "analyze": "webpack-bundle-analyzer dist/bundlesize-profile.json ./dist/site",
    "prettify": "prettier --write \"**/*.{js,ts,scss,json,yaml,md}\"",
    "precommit": "lint-staged",
    "postinstall": "patch-package",
    "plop": "plop",
    "generate": "sg wizard",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -o ./dist/storybook",
    "deploy": "rsync -avPx dist/site/ neom-webgl@eu.dev.monkapps.com:~/public/ --delete-after",
    "prepare": "yarn prepare:unix || yarn prepare:win",
    "prepare:unix": "while [ ! -d '.git' ] ; do cd .. ; done && husky install ui.frontend/.husky",
    "prepare:win": "powershell -Command \"while (!(Test-Path .git)) { cd .. }; husky install ui.frontend/.husky\""
  },
  "lint-staged": {
    "linters": {
      "**/*.{js,ts,scss,json,yaml,md}": [
        "prettier --write",
        "git add"
      ],
      "src/**/*.{js,ts}": [
        "eslint --ext .js,.ts --cache"
      ],
      "src/**/*.scss": [
        "stylelint --cache"
      ]
    }
  },
