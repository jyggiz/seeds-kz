


<section class="b-regionSlider{{#if theme}} t-{{theme}}{{/if}}"
         data-component="c05-region-slider"
         data-scroll-component
         {{#if activeItem}}data-active-item="{{activeItem}}"{{/if}}
         {{#if id}} id="{{id}}"{{/if}}
         data-lazy-component>

  {{> organism/o71-region-slider-navigation }}
  {{> organism/o72-region-slider-content }}
  {{#with intro}}
    <div class="b-regionSlider__intro">
      <div class="b-regionSlider__introBackground" data-region-intro-background>
        {{> molecule/m34-component-background background }}
      </div>
      <div class="b-regionSlider__introHeader" data-region-intro>
    {{> molecule/m18-paragraph content}}
    </div>
    </div>
  {{/with}}
</section>
