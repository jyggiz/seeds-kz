

<section class="b-contentGroup{{#if theme}} t-{{theme}}{{/if}} {{#if fullMinHeight}} -fullMinHeight{{/if}}"
  data-component="c09-content-group"
  {{#if scrollComponent}}data-scroll-component{{/if}}
  {{#if id}} id="{{id}}"{{/if}}
  {{#if (condition backgroundColor '||' padding)}}
         style="{{#with padding}}--component-block-padding: var(--{{#with start}}block-padding-{{this}}{{else}}container-margin{{/with}})
           {{#unless (condition start '===' end)}}var(--{{#with end}}block-padding-{{this}}{{else}}container-margin{{/with}}){{/unless}};{{/with}}
           {{#with backgroundColor}}--component-background-color: {{#unless (condition this '===' transparent)}}var(--color-{{this}}){{^}}transparent{{/unless}};{{/with}}"
  {{/if}}
  data-lazy-component
>
  {{#with background}}{{> molecule/m34-component-background disableTransition=true scrollComponent=true}}{{/with}}
  {{#with header}}
    <header class="b-contentGroup__header" data-content-group-header>
      {{> molecule/m04-component-header disableTransition=true scrollComponent=true }}
    </header>
  {{/with}}

  {{#with items}}
    <ul class="b-contentGroup__items" data-content-group-items>
      {{#each this}}
        <li class="b-contentGroup__item" data-content-group-item>
          {{#if (condition id '===' 'o53')}}{{> organism/o53-text-asset data scrollComponent=../scrollComponent }}{{/if}}
        </li>
      {{/each}}
    </ul>
  {{/with}}

  {{#with buttons }}
    <footer class="b-contentGroup__buttons" data-content-buttons>
      {{#each this}}
        {{> molecule/m02-button this }}
      {{/each}}
    </footer>
  {{/with}}
</section>
